<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/stores_graph"
    app:startDestination="@id/ScreenAFragment">

    <fragment
        android:id="@+id/ScreenAFragment"
        android:name="com.skoove.app.presentation.ScreenA.ScreenAFragment"
        android:label="ScreenA" >


        <action
            android:id="@+id/action_ScreenAFragment_to_ScreenB1Fragment"
            app:destination="@id/ScreenB1Fragment" />
        <action
            android:id="@+id/action_ScreenAFragment_to_ScreenB2Fragment"
            app:destination="@id/ScreenB2Fragment" />
        <action
            android:id="@+id/action_ScreenAFragment_to_ScreenB3Fragment"
            app:destination="@id/ScreenB3Fragment" />
        <action
            android:id="@+id/action_ScreenAFragment_to_ScreenC2Fragment2"
            app:destination="@id/ScreenC2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/ScreenB1Fragment"
        android:name="com.skoove.app.presentation.ScreenBx.ScreenB1Fragment"
        android:label="ScreenB1" >
        <action
            android:id="@+id/action_ScreenB1Fragment_to_ScreenC1Fragment"
            app:destination="@id/ScreenC1Fragment" />
    </fragment>
    <fragment
        android:id="@+id/ScreenB2Fragment"
        android:name="com.skoove.app.presentation.ScreenBx.ScreenB2Fragment"
        android:label="ScreenB2" >
        <action
            android:id="@+id/action_ScreenB2Fragment_to_ScreenC2ragment"
            app:destination="@id/ScreenC2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/ScreenB3Fragment"
        android:name="com.skoove.app.presentation.ScreenBx.ScreenB3Fragment"
        android:label="ScreenB3" >
        <action
            android:id="@+id/action_ScreenB3Fragment_to_ScreenC2ragment"
            app:destination="@id/ScreenC2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/ScreenC1Fragment"
        android:name="com.skoove.app.presentation.ScreenCx.ScreenC1Fragment"
        android:label="ScreenC1" >

        <action
            android:id="@+id/action_ScreenC1Fragment_to_ScreenDFragment"
            app:destination="@id/ScreenDFragment" />
    </fragment>
    <fragment
        android:id="@+id/ScreenC2Fragment"
        android:name="com.skoove.app.presentation.ScreenCx.ScreenC2Fragment"
        android:label="ScreenC2" >
        <action
            android:id="@+id/action_ScreenC2Fragment_to_ScreenDFragment"
            app:destination="@id/ScreenDFragment" />
    </fragment>


    <fragment
        android:id="@+id/ScreenDFragment"
        android:name="com.skoove.app.presentation.ScreenDFragment"
        android:label="ScreenD" />


</navigation>